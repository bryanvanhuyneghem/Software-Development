<Window x:Class="QuizGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizGUI"
        mc:Ignorable="d"
        Title="Quiz GUI" Height="400" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="240*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400*"/>
        </Grid.ColumnDefinitions>
        <local:LoadQuiz x:Name="loadQuiz" Grid.Row="0" Grid.Column="0"/>
        <local:ShowQuiz x:Name="showQuiz" Grid.Row="1" Grid.Column="0"/>
        
    </Grid>
</Window>
